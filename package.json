{
  "name": "react-mockups-vscode",
  "displayName": "react-mockups",
  "description": "View and test isolated components in React projects",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "jamsch",
    "email": "12927717+jamsch@users.noreply.github.com"
  },
  "repository": "https://github.com/jamsch/react-mockups-vscode",
  "publisher": "jamsch",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*.mockup.tsx"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ws": "^8.14.2"
  }
}
